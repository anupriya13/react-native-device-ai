{
  "name": "react-native-device-ai",
  "version": "1.0.0",
  "description": "Cross-platform React Native module that generates AI-powered insights and recommendations about the user's device.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "macos": "react-native run-macos --scheme WebviewExample --project-path example/macos",
    "start": "react-native start",
    "windows": "install-windows-test-app --project-directory example/windows && react-native run-windows --root example --arch x64",
    "ci:publish": "npx semantic-release",
    "lint": "npx tsc --noEmit && npx eslint ./src --ext .ts,.tsx,.js,.jsx",
    "build": "npx babel --extensions \".ts,.tsx\" --out-dir lib src",
    "test:windows": "yarn jest --setupFiles=./jest-setups/jest.setup.js",
    "test": "npx jest",
    "demo": "node standalone-demo.js",
    "demo:windows": "node windows-demo-standalone.js",
    "demo:interactive": "node working-example-demo.js",
    "demo:mcp": "node mcp-demo.js",
    "demo:os-specific": "node os-specific-mcp-demo.js"
  },
  "keywords": [
    "react-native",
    "device",
    "ai",
    "insights",
    "performance",
    "battery",
    "azure",
    "openai",
    "windows"
  ],
  "author": "Anupriya Verma",
  "license": "MIT",
  "peerDependencies": {
    "react-native": ">=0.70.0"
  },
  "dependencies": {
    "axios": "^1.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.0",
    "@babel/core": "^7.20.0",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.20.0",
    "@callstack/react-native-visionos": "0.73.8",
    "@react-native-community/cli": "^15.0.0",
    "@react-native/babel-preset": "0.79.0",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@rnx-kit/metro-config": "1.3.15",
    "@semantic-release/git": "7.0.16",
    "@types/invariant": "^2.2.30",
    "@types/jest": "^29.5.12",
    "@types/react": "19.1.13",
    "@types/selenium-webdriver": "4.0.9",
    "appium": "1.17.0",
    "eslint": "8.57.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react": "19.1.1",
    "react-native": "0.79.0",
    "react-native-macos": "0.79.0",
    "react-native-test-app": "3.7.2",
    "react-native-windows": "0.79.0",
    "selenium-appium": "1.0.2",
    "selenium-webdriver": "4.0.0-alpha.7",
    "semantic-release": "15.13.24",
    "typescript": "5.1.3",
    "winappdriver": "^0.0.7"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ],
    "moduleNameMapper": {
      "^react-native$": "<rootDir>/jest.setup.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anupriya13/react-native-device-ai.git"
  },
  "files": [
    "android/src",
    "android/build.gradle",
    "android/gradle.properties",
    "apple",
    "ios",
    "macos",
    "windows",
    "lib",
    "src",
    "index.js",
    "index.d.ts",
    "react-native-webview.podspec",
    "react-native.config.js"
  ],
  "codegenConfig": {
    "name": "ReactNativeDeviceAI",
    "type": "all",
    "jsSrcsDir": "./src",
    "android": {
      "javaPackageName": "com.reactnativecommunity.deviceai"
    },
    "ios": {
      "modulesProvider": {
        "ReactNativeDeviceAI": "ReactNativeDeviceAI"
      }
    },
    "windows": {
      "namespace": "ReactNativeDeviceAiCodegen",
      "outputDirectory": "windows/ReactNativeDeviceAi/codegen",
      "separateDataTypes": true
    }
  },
  "packageManager": "yarn@1.22.19",
  "react-native-windows": {
    "init-windows": {
      "name": "ReactNativeDeviceAi",
      "namespace": "ReactNativeDeviceAi",
      "template": "cpp-lib"
    }
  }
}
