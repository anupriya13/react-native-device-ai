{
  "name": "react-native-device-ai",
  "version": "1.0.0",
  "description": "Cross-platform React Native module that generates AI-powered insights and recommendations about the user's device.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "prepare": "npm run lint"
  },
  "keywords": [
    "react-native",
    "device",
    "ai",
    "insights",
    "performance",
    "battery",
    "azure",
    "openai",
    "windows"
  ],
  "author": "Anupriya Verma",
  "license": "MIT",
  "peerDependencies": {
    "react-native": ">=0.70.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.0.0"
  },
  "codegenConfig": {
    "name": "DeviceAISpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.deviceai"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anupriya13/react-native-device-ai.git"
  },
  "bugs": {
    "url": "https://github.com/anupriya13/react-native-device-ai/issues"
  },
  "homepage": "https://github.com/anupriya13/react-native-device-ai#readme",
  "jest": {
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "testMatch": ["**/__tests__/**/*.test.js"],
    "collectCoverageFrom": [
      "src/**/*.js",
      "index.js",
      "!src/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "testEnvironment": "node"
  }
}
