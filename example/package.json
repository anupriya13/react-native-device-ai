{
  "name": "react-native-device-ai-example",
  "version": "1.0.0",
  "description": "Example app demonstrating react-native-device-ai usage",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "windows": "npx @react-native-community/cli run-windows --release",
    "test:windows": "jest --config jest.config.windows.js",
    "postinstall": "node scripts/fix-react-native-windows.js",
    "fix-metro": "node scripts/fix-react-native-windows.js"
  },
  "keywords": [
    "react-native",
    "device-ai",
    "example"
  ],
  "author": "Anupriya Verma",
  "license": "MIT",
  "dependencies": {
    "react": "^19.0.0",
    "react-native": "^0.79.0",
    "react-native-device-ai": "file:../",
    "react-native-windows": "0.79.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^29.0.0",
    "eslint": "^7.32.0",
    "jest": "^29.0.0",
    "react-test-renderer": "19.0.0",
    "@rnx-kit/jest-preset": "^0.1.17",
    "@react-native/metro-config": "^0.79.0",
    "@react-native/babel-preset": "^0.79.0"
  },
  "resolutions": {
    "glob": "^10.0.0",
    "rimraf": "^5.0.0",
    "@xmldom/xmldom": "^0.8.0",
    "inflight": "^1.0.6"
  },
  "react-native-windows": {
    "init-windows": {
      "name": "ReactNativeDeviceAiExample",
      "namespace": "ReactNativeDeviceAiExample",
      "template": "cpp-app"
    }
  }
