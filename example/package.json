{
  "name": "react-native-device-ai-example",
  "version": "1.0.0",
  "description": "Example app demonstrating react-native-device-ai usage",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "windows": "echo \"Windows setup required. See README.md for React Native 0.79 Windows instructions.\" && react-native run-windows",
    "windows-init": "echo \"Using: yarn react-native init-windows --template cpp-lib\" && react-native init-windows --template cpp-lib",
    "demo": "cd .. && node standalone-demo.js",
    "demo-interactive": "cd .. && node working-example-demo.js",
    "autolink": "react-native autolink",
    "test": "cd .. && npm test"
  },
  "keywords": [
    "react-native",
    "device-ai",
    "example"
  ],
  "author": "Anupriya Verma",
  "license": "MIT",
  "dependencies": {
    "react": "^19.0.0",
    "react-native": "^0.79.0",
    "react-native-device-ai": "file:..",
    "react-native-windows": "^0.79.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "latest",
    "metro-react-native-babel-preset": "^0.76.0"
  },
  "setupInstructions": {
    "windows": [
      "Initialize Windows support: yarn react-native init-windows --template cpp-lib",
      "Run the app: npm run windows"
    ]
  },
  "codegenConfig": {
    "windows": {
      "namespace": "ReactNativeDeviceAiExampleCodegen",
      "outputDirectory": "windows/ReactNativeDeviceAiExample/codegen",
      "separateDataTypes": true
    }
  },
  "react-native-windows": {
    "init-windows": {
      "name": "ReactNativeDeviceAiExample",
      "namespace": "ReactNativeDeviceAiExample",
      "template": "cpp-lib"
    }
  }
}
